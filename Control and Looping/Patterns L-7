import java.util.*;
public class TestClass {
	 public static void main(String[] args) { 
       Scanner starPattern = new Scanner(System.in); 
       int a = starPattern.nextInt();
      for (int i = a; i >= 1; i--) {
          for (int j = i; j >=1; j--) {
            System.out.print(j + " ");
            }
        System.out.println();
      }
     }
}
