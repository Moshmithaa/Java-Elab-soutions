import java.util.*;
public class TestClass {
	 public static void main(String[] args) { 
	   Scanner scan = new Scanner(System.in);
       int a = scan.nextInt(); 
       String str = Integer.toString(a);
       for(int i=str.length()-1;i>=0;i--){
         System.out.print(" " + str.charAt(i));
       }
	}
}
