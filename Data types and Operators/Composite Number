import java.util.*;
public class TestClass {
	 public static void main(String[] args) { 
		Scanner composite = new Scanner(System.in);
       int a,b = 0;
       a = composite.nextInt();
       for(int i=2;i<a;i++){
         if(a%i==0)
         b++;
         break;
       }
       if(b==0){
         System.out.println("no");
       }else{
         System.out.println("yes");
       }
	}
}
