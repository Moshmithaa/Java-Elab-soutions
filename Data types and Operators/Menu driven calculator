import java.util.*;
public class TestClass {
	 public static void main(String[] args) { 
		Scanner calc = new Scanner(System.in);
       int a,b,c;
       a = calc.nextInt();
       b = calc.nextInt();
       c = calc.nextInt();
       switch(a){
         case 1:
           System.out.println(b + c);
           break;
         case 2:
           System.out.println(b - c);
           break;
         case 3:
           System.out.println(b / c);
           break;
         case 4: 
           System.out.println(b * c);
           break;
         case 5:
           System.out.println(b % c);
           break;
         default:
           System.out.println("Error");
           break;
       }
	}
}
