import java.util.*;
public class TestClass {
	 public static void main(String[] args) { 
		Scanner starPattern = new Scanner(System.in);
       int a = starPattern.nextInt();
       for (int i=1; i<=a; i++)
        {
            for (int j=1; j<=i; j++)
            {
                System.out.print("*");
            }
            System.out.println();
        }
	}
}
