import java.util.*;
class Sample{
  public static int n;
  public static void getInput(){
    Scanner scan = new Scanner(System.in);
    n = scan.nextInt();
  }
  public static void displayFib(){
    int a=0,b=1,c;
    System.out.print(b + " ");
    for(int i=1;i<n;i++){
      if(n>0){
        c = a + b;
        System.out.print( c + " ");
        a = b;
        b = c;
      }
    }
  }
}
public class TestClass {
	 public static void main(String[] args) { 
		Sample.getInput();
       Sample.displayFib();
	}
}
