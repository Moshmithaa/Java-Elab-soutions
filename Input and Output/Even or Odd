import java.util.*;
public class TestClass {
	 public static void main(String[] args) { 
		Scanner eo = new Scanner(System.in);
       int a = eo.nextInt();
       if(a % 2 == 0){
         System.out.println("Entered number is even");
       }else {
         System.out.println("Entered number is odd");
       }
	}
}
